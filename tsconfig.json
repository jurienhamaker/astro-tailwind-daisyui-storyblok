{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"types": ["@astrojs/image/client"],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@utils/*": ["src/utils/*"],
			"@store/*": ["src/store/*"]
		}
	}
}
